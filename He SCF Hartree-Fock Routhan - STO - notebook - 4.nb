Cell[CellGroupData[{

Cell["Calculation of Two-Electron Integrals", "Subsubsection",
 CellChangeTimes->{{3.8180522199192286`*^9, 
  3.818052232364215*^9}},ExpressionUUID->"9ee2d67f-4467-49fd-906b-\
a6c3ba024c65"],

Cell[TextData[{
 "Defining a function to calculate Integrals of the form:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["\[Phi]", "i"], "[", 
        RowBox[{
         SubscriptBox["r", "1"], "-", 
         SubscriptBox["R", "A"]}], "]"}], "\[Conjugate]"}], 
      RowBox[{
       SubscriptBox["\[Phi]", "j"], "[", 
       RowBox[{
        SubscriptBox["r", "1"], "-", 
        SubscriptBox["R", "B"]}], "]"}], 
      RowBox[{"(", 
       FractionBox["1", 
        SubscriptBox["r", "12"]], ")"}], 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Phi]", "k"], "[", 
        RowBox[{
         SubscriptBox["r", "2"], "-", 
         SubscriptBox["R", "\[CapitalCAcute]"]}], "]"}], "\[Conjugate]"}], 
      RowBox[{
       SubscriptBox["\[Phi]", "l"], "[", 
       RowBox[{
        SubscriptBox["r", "2"], "-", 
        SubscriptBox["R", "D"]}], "]"}], " ", 
      SuperscriptBox[
       SubscriptBox["r", "1"], "2"], 
      SuperscriptBox[
       SubscriptBox["r", "2"], "2"], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["r", "1"]}], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["r", "2"]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "f8c5e5ae-50c6-42e1-8123-b1af0539726a"],
 "\n\nA,B,\[CapitalCAcute],D are exponents of the four \[Phi] GTOs ; RAB2 and \
RCD2 are square of the distance between the corresponding center of \[Phi] \
GTOs ; and P and Q are calculated using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["R", "P"], "=", 
    FractionBox[
     RowBox[{
      RowBox[{"A", " ", 
       SubscriptBox["R", "A"]}], "+", 
      RowBox[{"B", " ", 
       SubscriptBox["R", "B"]}]}], 
     RowBox[{"A", "+", "B"}]]}], TraditionalForm]],ExpressionUUID->
  "0d4dcd92-2024-484d-a466-5c80e8755fbc"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["R", "Q"], "=", 
    FractionBox[
     RowBox[{
      RowBox[{"\[CapitalCAcute]", " ", 
       SubscriptBox["R", "\[CapitalCAcute]"]}], "+", 
      RowBox[{"D", " ", 
       SubscriptBox["R", "D"]}]}], 
     RowBox[{"\[CapitalCAcute]", "+", "D"}]]}], TraditionalForm]],
  ExpressionUUID->"5ff94074-6e04-4f9d-90a2-e9a8947f9da2"],
 " , respectively."
}], "Text",
 CellChangeTimes->{{3.818052245125225*^9, 3.8180523166212463`*^9}, {
  3.8180523623162427`*^9, 3.818052387710249*^9}, {3.818052447793228*^9, 
  3.81805254081623*^9}, {3.818052571959319*^9, 3.818052599641317*^9}, {
  3.818052634898366*^9, 3.818052820665428*^9}, {3.8180528519298067`*^9, 
  3.8180529743327646`*^9}},ExpressionUUID->"05e69bde-0339-4f2a-9673-\
d053d95e648f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"twoEfunc", "[", 
   RowBox[{"i_", ",", "j_", ",", "k_", ",", "l_"}], "]"}], ":=", 
  TagBox[GridBox[{
     {"\[Piecewise]", GridBox[{
        {
         RowBox[{
          RowBox[{"5", "/", "8"}], " ", 
          RowBox[{
          "\[Zeta]", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}], 
         RowBox[{"i", "\[Equal]", "j", "\[Equal]", "k", "\[Equal]", "l"}]},
        {
         FractionBox[
          RowBox[{
           RowBox[{
            SuperscriptBox[
             RowBox[{
             "\[Zeta]", "\[LeftDoubleBracket]", "i", 
              "\[RightDoubleBracket]"}], "4"], 
            SuperscriptBox[
             RowBox[{
             "\[Zeta]", "\[LeftDoubleBracket]", "k", 
              "\[RightDoubleBracket]"}], "1"]}], "+", 
           RowBox[{"4", 
            SuperscriptBox[
             RowBox[{
             "\[Zeta]", "\[LeftDoubleBracket]", "i", 
              "\[RightDoubleBracket]"}], "3"], 
            SuperscriptBox[
             RowBox[{
             "\[Zeta]", "\[LeftDoubleBracket]", "k", 
              "\[RightDoubleBracket]"}], "2"]}], "+", 
           RowBox[{
            SuperscriptBox[
             RowBox[{
             "\[Zeta]", "\[LeftDoubleBracket]", "i", 
              "\[RightDoubleBracket]"}], "1"], 
            SuperscriptBox[
             RowBox[{
             "\[Zeta]", "\[LeftDoubleBracket]", "k", 
              "\[RightDoubleBracket]"}], "4"]}], "+", 
           RowBox[{"4", 
            SuperscriptBox[
             RowBox[{
             "\[Zeta]", "\[LeftDoubleBracket]", "i", 
              "\[RightDoubleBracket]"}], "2"], 
            SuperscriptBox[
             RowBox[{
             "\[Zeta]", "\[LeftDoubleBracket]", "k", 
              "\[RightDoubleBracket]"}], "3"]}]}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox[
              RowBox[{
              "\[Zeta]", "\[LeftDoubleBracket]", "i", 
               "\[RightDoubleBracket]"}], "1"], "+", 
             SuperscriptBox[
              RowBox[{
              "\[Zeta]", "\[LeftDoubleBracket]", "k", 
               "\[RightDoubleBracket]"}], "1"]}], ")"}], "4"]], 
         RowBox[{
