  3.818055543754175*^9}},ExpressionUUID->"451ffd2a-781e-4152-af3d-\
47e918e45f0d"],

Cell[BoxData[
 RowBox[{"While", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"Norm", "[", 
      RowBox[{
       RowBox[{"P", "-", "newP"}], "//", "N"}], "]"}], "\[GreaterEqual]", 
     "plim"}], "&&", 
    RowBox[{"iter", "\[LessEqual]", "maxIter"}]}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"iter", "+=", "1"}], ";", "\[IndentingNewLine]", 
    RowBox[{"P", "=", "newP"}], ";", "\[IndentingNewLine]", 
    RowBox[{"G", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"\[Lambda]", "=", "1"}], "numBasis"], 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"\[Sigma]", "=", "1"}], "numBasis"], 
         RowBox[{
          RowBox[{"P", "\[LeftDoubleBracket]", 
           RowBox[{"\[Lambda]", ",", "\[Sigma]"}], "\[RightDoubleBracket]"}], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"\[Mu]", ",", 
              RowBox[{"\[Nu]", "|", "\[Sigma]"}], ",", "\[Lambda]"}], ")"}], "-", 
            RowBox[{
             FractionBox["1", "2"], 
             RowBox[{"(", 
              RowBox[{"\[Mu]", ",", 
               RowBox[{"\[Lambda]", "|", "\[Sigma]"}], ",", "\[Nu]"}], 
              ")"}]}]}], ")"}]}]}]}], ",", 
       RowBox[{"{", 
        RowBox[{"\[Mu]", ",", "numBasis"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"\[Nu]", ",", "numBasis"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"F", "=", 
     RowBox[{
      SuperscriptBox["H", "core"], "+", "G"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     SuperscriptBox["F", "\[Prime]"], "=", 
     RowBox[{
      RowBox[{"MatrixPower", "[", 
       RowBox[{"S", ",", 
        FractionBox[
         RowBox[{"-", "1"}], "2"]}], "]"}], ".", "F", ".", 
      RowBox[{"MatrixPower", "[", 
       RowBox[{"S", ",", 
        FractionBox[
         RowBox[{"-", "1"}], "2"]}], "]"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       SuperscriptBox["\[GothicCapitalC]", "\[Prime]"], ",", 
       "\[CurlyEpsilon]"}], "}"}], "=", 
     RowBox[{
      RowBox[{"JordanDecomposition", "[", 
       SuperscriptBox["F", "\[Prime]"], "]"}], "/.", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"c_", ",", "e_"}], "}"}], "\[RuleDelayed]", 
       RowBox[{"{", 
        RowBox[{"c", ",", 
         RowBox[{"Reverse", "/@", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"Reverse", "/@", "e"}], ")"}], "\[Transpose]"}], 
           ")"}]}]}], "}"}]}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"\[CapitalCAcute]", "=", 
     RowBox[{
      RowBox[{"MatrixPower", "[", 
       RowBox[{"S", ",", 
        FractionBox[
         RowBox[{"-", "1"}], "2"]}], "]"}], ".", 
      SuperscriptBox["\[GothicCapitalC]", "\[Prime]"]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"\[CapitalCAcute]rot", "=", 
     RowBox[{"Reverse", "/@", "\[CapitalCAcute]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"newP", "=", 
     RowBox[{"2", 
      RowBox[{"Table", "[", 
       StyleBox[
        RowBox[{
         RowBox[{
          UnderoverscriptBox["\[Sum]", 
           RowBox[{"a", "=", "1"}], 
           FractionBox["numElectrons", "2"]], 
          RowBox[{
           RowBox[{
            RowBox[{"\[CapitalCAcute]rot", "\[LeftDoubleBracket]", 
             RowBox[{"\[Mu]", ",", "a"}], "\[RightDoubleBracket]"}], 
            "\[Conjugate]"}], "*", 
           RowBox[{"\[CapitalCAcute]rot", "\[LeftDoubleBracket]", 
            RowBox[{"\[Nu]", ",", "a"}], "\[RightDoubleBracket]"}]}]}], ",", 
         RowBox[{"{", 
          RowBox[{"\[Mu]", ",", "numBasis"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[Nu]", ",", "numBasis"}], "}"}]}],
        FontWeight->"Bold"], 
       StyleBox["]",
        FontWeight->"Bold"]}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"ElecE", "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", "2"], "P", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["H", "core"], "+", "F"}], ")"}]}], " ", "//", 
      RowBox[{
       RowBox[{"Total", "[", 
        RowBox[{"#", ",", "2"}], "]"}], "&"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"TotalE", "=", 
     RowBox[{"ElecE", "+", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"numElectrons", "\[GreaterEqual]", "2"}], ",", 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"\[Mu]", "=", "1"}], "numAtoms"], 
         RowBox[{
          UnderoverscriptBox["\[Sum]", 
           RowBox[{"\[Nu]", "=", 
            RowBox[{"\[Mu]", "+", "1"}]}], "numAtoms"], 
          RowBox[{"(", 
           FractionBox[
            RowBox[{
             RowBox[{
             "Z", "\[LeftDoubleBracket]", "\[Mu]", "\[RightDoubleBracket]"}], 
             RowBox[{
             "Z", "\[LeftDoubleBracket]", "\[Nu]", 
              "\[RightDoubleBracket]"}]}], 
            SuperscriptBox[
             RowBox[{"Norm", "[", 
              RowBox[{
               RowBox[{
               "nucleiCoord", "\[LeftDoubleBracket]", "\[Mu]", 
                "\[RightDoubleBracket]"}], "-", 
               RowBox[{
               "nucleiCoord", "\[LeftDoubleBracket]", "\[Nu]", 
                "\[RightDoubleBracket]"}]}], "]"}], 
             RowBox[{"2", " "}]]], ")"}]}]}], ",", "0"}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"AppendTo", "[", 
     RowBox[{"results", ",", 
      RowBox[{"\[LeftAssociation]", 
       RowBox[{"iter", "\[Rule]", 
        RowBox[{"AssociationThread", "[", 
         RowBox[{
          RowBox[{"{", 
           
           RowBox[{"\"\<P\>\"", ",", "\"\<G\>\"", ",", "\"\<F\>\"", 
            ",", "\"\<\!\(\*SuperscriptBox[\(F\), \(\[Prime]\)]\)\>\"", 
            ",", "\"\<\[CapitalCAcute]\>\"", ",", "\"\<\[CurlyEpsilon]\>\"", 
            ",", "\"\<\!\(\*SubscriptBox[\(E\), \(elec\)]\)\>\"", 
            ",", "\"\<\!\(\*SubscriptBox[\(E\), \(total\)]\)\>\""}], "}"}], "\[Rule]", 
          RowBox[{"MatrixForm", "/@", 
           RowBox[{"{", 
            RowBox[{"P", ",", "G", ",", "F", ",", 
             SuperscriptBox["F", "\[Prime]"], ",", "\[CapitalCAcute]", ",", 
             "\[CurlyEpsilon]", ",", "ElecE", ",", "TotalE"}], "}"}]}]}], 
         "]"}]}], "\[RightAssociation]"}]}], "]"}], ";"}]}], 
  "\[IndentingNewLine]", "]"}]], "Input",
