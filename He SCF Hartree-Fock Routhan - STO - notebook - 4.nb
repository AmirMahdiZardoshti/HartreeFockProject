Cell[CellGroupData[{

Cell[TextData[{
 "Function for Calculating ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["S", "ij"], "=", 
   RowBox[{"\[Integral]", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["\[Phi]", "i"], "[", 
       RowBox[{"r", "-", 
        SubscriptBox["R", "i"]}], "]"}], "\[Conjugate]"}], 
     RowBox[{
      SubscriptBox["\[Phi]", "j"], "[", 
      RowBox[{"r", "-", 
       SubscriptBox["R", "j"]}], "]"}], 
     RowBox[{"\[DifferentialD]", "r"}]}]}]}]],ExpressionUUID->
  "8c8b33dd-193f-4a35-8bb7-9b15528a98fc"],
 " for ",
 StyleBox["GTO (not STO)",
  FontWeight->"Bold"]
}], "Subsubsection",
 CellChangeTimes->{{3.81804421896108*^9, 3.818044223264319*^9}, {
  3.8180450591530156`*^9, 3.818045064353013*^9}, {3.8180452254889975`*^9, 
  3.818045301757025*^9}, {3.818045501205024*^9, 3.8180455480935435`*^9}, {
  3.818132543419037*^9, 
  3.8181325464666615`*^9}},ExpressionUUID->"40582635-0fc8-44b8-8cd5-\
52cf016e94dc"],

Cell["\<\
A and B are exponents of GTO and RAB2 is distance between the center of basis \
set A to center of basis set B :\
\>", "Text",
 CellChangeTimes->{{3.818045586025779*^9, 
  3.818045666380754*^9}},ExpressionUUID->"5087158f-3ccd-4466-ad2e-\
bdf41e86a7e3"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Sfunc", "[", 
   RowBox[{"A_", ",", "B_", ",", "RAB2_"}], "]"}], ":=", 
  RowBox[{
   SuperscriptBox[
    RowBox[{"(", 
     FractionBox["\[Pi]", 
      RowBox[{"A", "+", "B"}]], ")"}], 
    FractionBox["3", "2"]], 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "A"}], " ", "B", 
     FractionBox["RAB2", 
      RowBox[{"A", "+", "B"}]]}]]}]}]], "Input",
 CellLabel->"In[27]:=",ExpressionUUID->"b4fc2d6d-5ce2-41f2-8a6e-4443f71492b6"]
